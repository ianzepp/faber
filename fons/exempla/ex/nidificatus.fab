# Nested ex...pro loops

incipit {
    # Nested array iteration
    fixum rows = [1, 2, 3]
    fixum cols = ["A", "B", "C"]

    ex rows fixum row {
        ex cols fixum col {
            scribe row, col
        }
    }

    # Multiplication table
    ex 1..4 fixum i {
        ex 1..4 fixum j {
            scribe i, "*", j, "=", i * j
        }
    }

    # Nested ranges
    ex 0..3 fixum x {
        ex 0..3 fixum y {
            scribe x, y
        }
    }
}
